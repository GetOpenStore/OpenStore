{
  "name": "openstore",
  "productName": "OpenStore",
  "version": "0.1.2",
  "description": "An open app store",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .js,.jsx.ts,.tsx .",
    "install-for-arm64": "npm i --target_arch=arm64",
    "postinstall-for-arm64": "npx electron-rebuild --arch arm64",
    "make-for-arm64": "npm run install-for-arm64 && electron-forge make --arch arm64",
    "install-for-x86_64": "npm i --target_arch=x64",
    "postinstall-for-x86_64": "npx electron-rebuild --arch x64",
    "make-for-x86_64": "npm run install-for-x86_64 && electron-forge make --arch x64"
  },
  "keywords": [],
  "author": {
    "name": "Ian Gregory"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "GetOpenStore",
              "name": "OpenStore"
            }
          }
        }
      ],
      "packagerConfig": {
        "icon": "./icons/openstore-app-icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "openstore"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devContentSecurityPolicy": "default-src 'self' 'unsafe-inline' data: https://cdn.jsdelivr.net https://fonts.googleapis.com https://fonts.gstatic.com https://formulae.brew.sh; script-src 'self' 'unsafe-eval' 'unsafe-inline' data: https://cdn.jsdelivr.net; img-src 'self' data: https://99designs-blog.imgix.net",
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "index.html",
                  "js": "renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "preload/preload.ts"
                  }
                }
              ]
            }
          },
          "@electron-forge/plugin-electronegativity",
          {
            "isSarif": true
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-electronegativity": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.61",
    "@types/better-sqlite3": "^7.4.2",
    "@types/bootstrap": "^5.1.8",
    "@types/color": "^3.0.2",
    "@types/node": "^17.0.5",
    "@types/shell-quote": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "css-loader": "^6.5.1",
    "electron": "16.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "lit-css-loader": "^1.2.1",
    "node-loader": "^2.0.0",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "better-sqlite3": "^7.4.5",
    "bootstrap": "^5.1.3",
    "color": "^4.1.0",
    "darkreader": "^4.9.43",
    "electron-context-menu": "^3.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "lit": "^2.0.2",
    "node-pty": "^0.10.1",
    "shell-quote": "^1.7.3",
    "xterm": "^4.16.0",
    "xterm-addon-fit": "^0.5.0"
  }
}
